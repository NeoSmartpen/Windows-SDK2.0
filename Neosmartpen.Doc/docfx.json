{
  "metadata": [
    {
      "src": [
        {
          "src": "../Neosmartpen.Net",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "shouldSkipMarkup": true,
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "modern",
      "default"
    ],
    "globalMetadata": {
      "_appName": "Neo smartpen SDK for Windows Platform",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "pdf": false
    }
  }
}